<div class="search_result">
	<table class="sortable" id="files">
		<tr class="sorttop">
			<th>Name</th>
			<th class="date">Geändert</th>
			<th class="numeric">Größe</th>
			<th>Type</th></tr>
		</tr>
% tags.each do |tag|
		<tr>
			<td><a href="<%= webroot %>/classic/<%= tag.to_html %>"><%= tag.to_html %></a></td>
			<td></td>
			<td class="size">4.0K</td>
			<td>application/x-directory</td>
		</tr>
% end
	</table>
</div>
